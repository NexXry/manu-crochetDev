                            </svg>
                            </span>
                        </a>
                        <a class="invisible lg:visible md:visible text-gray-900 hover:bg-orange4 hover:text-white rounded cursor-pointer pl-2" href="{{ path("app_logout") }}">
                            <i class="fas fa-sign-out-alt fa-2x"></i>
                        </a>
                        {%  else %}
                            {% if is_granted('ROLE_CHEF') and app.user is not empty%}
                                <a class="invisible lg:visible md:visible text-gray-900 hover:bg-orange4 hover:text-white rounded cursor-pointer pr-2" href="{{ path("backoffice") }}">
