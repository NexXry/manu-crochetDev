            <div class="space-x-8 xl:space-x-12 sm:flex items-center justify-between hidden md:px-0">
                <a href="{{ path("index") }}" class="text-lg py-1 px-2 hover:bg-orange4 hover:text-white rounded text-gray-900 font-semibold cursor-pointer">ACCUEIL</a>
                <a href="{{ path("boutique") }}" class="text-lg py-1 px-2 hover:bg-orange4 hover:text-white rounded text-gray-900 font-semibold cursor-pointer">BOUTIQUE</a>
            </div>

            <div class="flex mr-8">
                <div class="flex items-center">
                    {#  app.user.isVerified == true  #}
                    {% if is_granted('ROLE_CLIENT') and app.user is not empty %}
                        <a href="{{ path("app_profil") }}">
                            <svg xmlns="http://www.w3.org/2000/svg" class="invisible lg:visible md:visible h-8 w-8 text-gray-900 hover:bg-orange4 hover:text-white rounded  cursor-pointer" fill="none"
                                 viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                            </svg>
                        </a>
                        <a href="{{ path("panier") }}" class="relative ml-2">
                            <svg xmlns="http://www.w3.org/2000/svg" class="invisible lg:visible md:visible h-8 w-8 text-gray-900 hover:bg-orange4 hover:text-white rounded cursor-pointer" fill="none"
                                 viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
